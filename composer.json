{
  "name": "frogsystem/spawn",
  "description": "Spawn is a simple and lightweight implementation of an IoC application container and fully compatible with container-interop.",
  "keywords": ["container", "container-interop", "IoC", "DIC"],
  "homepage": "https://github.com/frogsystem/spawn",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Moritz Kornher",
      "email": "mail@moritzkornher.de"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "container-interop/container-interop": "1.1.0"
  },
  "provide": {
    "container-interop/container-interop-implementation": "1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Frogsystem\\Spawn\\": "src/frogsystem/spawn/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "4.6.6"
  }
}
